            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/gi2mo/ns#Description"/>
                    <rdf:Description rdf:about="http://purl.org/gi2mo/ns#Idea"/>
