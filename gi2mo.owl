                    <rdf:Description rdf:about="http://purl.org/gi2mo/ns#SubmissionMethod"/>
                    <rdf:Description rdf:about="http://purl.org/gi2mo/ns#UserGroup"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Deprecated. Use dcterms:title instead.</rdfs:comment>
        <rdfs:comment xml:lang="en">Describes the title of an entity (e.g. idea title, idea contest tile, review title etc.).</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://purl.org/gi2mo/ns</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">title</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
    </owl:DatatypeProperty>
    


